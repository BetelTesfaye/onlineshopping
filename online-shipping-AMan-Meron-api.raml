            example:
              {
                "uid": 1010
              }
      201:
        body:
          application/json:
            description: Created 
      401:
        body:
          application/json:
            description: Unauthorized
      403:
        body:
          application/json:
            description: Forbidden
      404:
        body:
          application/json:
            description: Not Found                        
              
  /{id}:
    uriParameters:
      id:
        required: true
    get:
