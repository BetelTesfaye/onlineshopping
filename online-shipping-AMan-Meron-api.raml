            } 
      403:
        body:
          application/json:
            example: 
              {
             Forbidden
            }      
      404:
        body:
          application/json:
            example: 
              {
             Not Found
            }      
                   
  post:
    body:
      application/json:
        example:
          {
            "email": "craft@gmail.com",
            "username": "craft"
          }
